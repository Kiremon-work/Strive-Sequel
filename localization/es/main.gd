BOLTHAR_HARA_OPT_REBELS = "¿Estás realmente ayudando a los rebeldes?",
BOLTHAR_HARA_OPT_KING = "¿Cuál es tu arreglo con el rey?",
BOLTHAR_HARA_OPT_TREATMENT = "¿Cómo te han tratado los rebeldes?",
BOLTHAR_HARA_OPT_TOKING = "Necesito llevarte de regreso al rey",
	BOLTHAR_HARA_REPLY_REBELS = """Adjusting her goggles, Hara shrugs nonchalantly, a smirk plays across her lips.

